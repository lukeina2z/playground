{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ADOT Debug",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "debug"
            ],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "skipFiles": [
                // "<node_internals>/**"
            ],
            "type": "node",
            "env": {
                "NODE_OPTIONS": "--require @aws/aws-distro-opentelemetry-node-autoinstrumentation/register",
                "OTEL_EXPORTER_OTLP_TRACES_TIMEOUT": "3",
                // "OTEL_BSP_SCHEDULE_DELAY": "1",
                // "OTEL_BSP_MAX_QUEUE_SIZE": "1",
                "OTEL_METRICS_EXPORTER": "none",
                "OTEL_LOGS_EXPORTER": "none",
                // "OTEL_TRACES_EXPORTER": "otlp,console",
                "OTEL_AWS_APPLICATION_SIGNALS_ENABLED": "true",
                "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
                "OTEL_TRACES_SAMPLER": "always_on", // "xray",
                // "OTEL_TRACES_SAMPLER_ARG": "endpoint=http://localhost:2000",
                "OTEL_AWS_APPLICATION_SIGNALS_EXPORTER_ENDPOINT": "http://localhost:4316/v1/metrics",
                "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://localhost:4316/v1/traces",
                // "OTEL_RESOURCE_ATTRIBUTES": "service.name=nodejs-ADOT-withAPI",
                "OTEL_RESOURCE_ATTRIBUTES": "service.name=nodejs-ADOT-withoutAPI-butImport",
                // "OTEL_SERVICE_NAME": "nodejs-ADOT",
                // "OTEL_NODE_RESOURCE_DETECTORS": "env,host,os,process",
                // // "NODE_OPTIONS": "--require @opentelemetry/auto-instrumentations-node/register",
                // // "OTEL_LOG_LEVEL": "all",
                // // "OTEL_RESOURCE_ATTRIBUTES": "service.name=xy-nodejs-svc-BAR",
                // "OTEL_METRICS_EXPORTER": "none",
                // "OTEL_LOGS_EXPORTER": "none",
                // // "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:4318",
                // "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://localhost:4316/v1/traces",
                // "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
                // "OTEL_TRACES_SAMPLER": "always_on",
                // "OTEL_TRACES_SAMPLER_ARG": "endpoint=http://localhost:2000",
                // "OTEL_AWS_APPLICATION_SIGNALS_ENABLED": "true",
                // "OTEL_AWS_APPLICATION_SIGNALS_EXPORTER_ENDPOINT": "http://localhost:4316/v1/metrics",
            },
            "sourceMaps": true,
            "console": "integratedTerminal"
        }
    ]
}