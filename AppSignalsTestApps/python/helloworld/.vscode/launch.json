    "version": "0.2.0",
    "configurations": [
        {
            "name": "dbg-m-http.server",
            "type": "debugpy",
            "request": "launch",
            "module": "http.server",
            // C:\Users\Administrator\AppData\Local\Programs\Python\Python313\Lib\http
            "stopOnEntry": true,
            "args": [
                "8888"
            ],
            "justMyCode": false
        },
        {
            "name": "dbg-m-pip",
            "type": "debugpy",
            "request": "launch",
            "module": "pip",
            // C:\Users\Administrator\AppData\Local\Programs\Python\Python313\Lib\site-packages\pip
            "stopOnEntry": true,
            "args": [
                "inspect"
            ],
            "justMyCode": false
        },
        {
            "name": "hello-world",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/helloworld.py",
            "console": "integratedTerminal",
            "args": []
            // "args": "${command:pickArgs}"
        },
        {
            "name": "adot-test",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/adot-test.py",
            "console": "integratedTerminal",
            "args": []
            // "args": "${command:pickArgs}"
        },
        {
            "name": "OTel-Agent-Test",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/v-otel/bin/opentelemetry-instrument",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/v-otel/bin/python",
                "${workspaceFolder}/adot-test.py"
            ],
            "env": {
                "OTEL_RESOURCE_ATTRIBUTES": "service.name=python-otel-launchjson",
                "OTEL_TRACES_EXPORTER": "otlp",
                "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://xyz-jaeger-100:4317/v1/traces"
            }
        },
        {
            "name": "ADOT-Agent-Test",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/v-adot/bin/opentelemetry-instrument",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/v-adot/bin/python",
                "${workspaceFolder}/adot-test.py"
            ],
            "env": {
                "OTEL_METRICS_EXPORTER": "none",
                "OTEL_LOGS_EXPORTER": "none",
                "OTEL_AWS_APPLICATION_SIGNALS_ENABLED": "true",
                "OTEL_PYTHON_DISTRO": "aws_distro",
                "OTEL_PYTHON_CONFIGURATOR": "aws_configurator",
                "OTEL_EXPORTER_OTLP_PROTOCOL": "grpc",
                "OTEL_TRACES_SAMPLER": "xray",
                "OTEL_TRACES_SAMPLER_ARG": "endpoint=http://localhost:2000",
                "OTEL_AWS_APPLICATION_SIGNALS_EXPORTER_ENDPOINT": "http://localhost:4316/v1/metrics",
                "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://xyz-jaeger-100:4317/v1/traces",
                "OTEL_RESOURCE_ATTRIBUTES": "service.name=python-ADOT-Launch.json"
            }
        }
    ]
}