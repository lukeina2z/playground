    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "ADOT-springboot-ec2-fatjar",
            "request": "launch",
            "mainClass": "com.example.demo.DemoApplication",
            "args": [],
            "env": {
                "OTEL_RESOURCE_ATTRIBUTES": "service.name=ADOT-SB-EC2-fatjar",
                "JAVA_TOOL_OPTIONS": " -javaagent:/Users/lukezha/github/otel-java/wk-adot-pr/adot-java-pr-foo-2.11/otelagent/build/libs/aws-opentelemetry-agent-2.11.0-SNAPSHOT.jar",
                // "AWS_LAMBDA_FUNCTION_NAME": "Great-Lambda-Fn-XXX",
                "OTEL_INSTRUMENTATION_SERVLET_ENABLED": "true",
                "OTEL_JAVAAGENT_DEBUG": "true",
                "OTEL_LOG_LEVEL": "all",
                "OTEL_METRICS_EXPORTER": "none",
                // "OTEL_LOGS_EXPORTER": "none",
                "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:4318",
                "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://localhost:4318/v1/traces",
                // "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://localhost:4316/v1/traces",
                // "OTEL_METRIC_EXPORT_INTERVAL": "2000",
                // "OTEL_EXPORTER_OTLP_PROTOCOL": "http/json",
                "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
                "OTEL_TRACES_SAMPLER": "always_on",
                "OTEL_TRACES_SAMPLER_ARG": "endpoint=http://localhost:2000",
                "OTEL_AWS_APPLICATION_SIGNALS_ENABLED": "true"
                // "OTEL_AWS_APPLICATION_SIGNALS_EXPORTER_ENDPOINT": "http://localhost:4316/v1/metrics"
            },
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "vmArgs": [
                "-cp",
                "${workspaceFolder}/build/libs/adot-springboot-ec2-1.0.1-SNAPSHOT-all.jar"
                // "-javaagent:/Users/lukezha/github/otel-java/wk-adot-pr/adot-java-pr-foo/otelagent/build/libs/aws-opentelemetry-agent-2.11.0-SNAPSHOT.jar"
            ]
        },
        {
