    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch with OTel",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/build/client.js",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/build/**/*.js",
                "/Users/lukezha/github/otel-js/working/opentelemetry-js-contrib-lkmcp/packages/auto-instrumentations-node/build/**/*.js",
                "/Users/lukezha/github/otel-js/working/opentelemetry-js-contrib-lkmcp/packages/instrumentation-aws-sdk/build/**/*.js",
                "/Users/lukezha/github/otel-js/working/opentelemetry-js-contrib-lkmcp/packages/instrumentation-mcp/build/**/*.js",
                "/Users/lukezha/github/otel-js/working/opentelemetry-js-contrib-lkmcp/node_modules/@opentelemetry/instrumentation/build/**/*.js",
                "/Users/lukezha/github/otel-js/working/opentelemetry-js-contrib-lkmcp/node_modules/@opentelemetry/instrumentation-http/build/**/*.js",
            ],
            "env": {
                "OTEL_SERVICE_NAME": "mcp-client-LaunchJson",
                "OTEL_NODE_RESOURCE_DETECTORS": "aws,env,host,os,process",
                // "OTEL_LOG_LEVEL": "warn",
                "OTEL_TRACES_EXPORTER": "otlp",
                "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://127.0.0.1:4318/v1/traces"
            },
            "runtimeArgs": [
                "--require",
                "@opentelemetry/auto-instrumentations-node/register",
            ],
            "console": "integratedTerminal"
        }
    ]
}