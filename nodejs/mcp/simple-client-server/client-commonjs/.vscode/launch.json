{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch with OTel",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/build/client.js",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/build/**/*.js"
            ],
            "env": {
                "OTEL_SERVICE_NAME": "Mcp-Client-CJS-LaunchJson",
                "OTEL_LOG_LEVEL": "all",
                "OTEL_TRACES_EXPORTER": "otlp",
                "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://xyz-jaeger-100:4318/v1/traces"
            },
            "runtimeArgs": [
                "--import",
                "@opentelemetry/auto-instrumentations-node/register",
                "--experimental-loader=@opentelemetry/instrumentation/hook.mjs"
            ]
        }
    ]
}